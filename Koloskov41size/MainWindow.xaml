<Window x:Class="Koloskov41size.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Koloskov41size"
        mc:Ignorable="d"
        Title="Koloskov41Size" MinHeight="600" MinWidth="900" FontFamily="Comic Sans MS" Icon="res/icon.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Border Grid.Row="0" Background="#FFCC99"></Border>
        <Border Grid.Row="2" Background="#FFCC99"></Border>
        <Image Source="res/logo.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="41 размер" HorizontalAlignment="Center" FontSize="30" VerticalAlignment="Center"></TextBlock>
        <Button Name="BtnBack" Content="Назад" HorizontalAlignment="Right" Click="BtnBack_Click"></Button>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
